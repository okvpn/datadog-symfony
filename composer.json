{
    "name":"okvpn/datadog-symfony",
    "type":"symfony-bundle",
    "license":"MIT",
    "description": "Symfony Datadog integration",
    "keywords": ["DogStatsD", "datadog-symfony", "php-datadog"],
    "authors": [
        {
            "name": "Uladzimir Tsykun",
            "homepage": "https://github.com/vtsykun",
            "email": "vtsykun@okvpn.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Okvpn\\Bundle\\DatadogBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "Okvpn\\Bundle\\DatadogBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php":">=7.2",
        "symfony/framework-bundle": "^4.0 || ^5.0 || ^6.0",
        "symfony/yaml": "^4.0 || ^5.0 || ^6.0",
        "graze/dog-statsd": "^0.4 || ^1.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^1.6.10 || ^2.0.6",
        "doctrine/orm": "2.5.11 || ^2.6.3 || ^2.7.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.0 || ^5.0 || ^6.0",
        "symfony/console": "^4.0 || ^5.0 || ^6.0",
        "symfony/security-bundle": "^4.0 || ^5.0 || ^6.0",
        "symfony/phpunit-bridge": "^4.0 || ^5.0 || ^6.0",
        "symfony/var-dumper": "^4.0 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^3.2.0"
    }
}
