{
    "name":"okvpn/datadog-symfony",
    "type":"symfony-bundle",
    "license":"MIT",
    "description": "Symfony Datadog integration",
    "keywords": ["DogStatsD", "datadog-symfony", "php-datadog"],
    "authors": [
        {
            "name": "Uladzimir Tsykun",
            "homepage": "https://github.com/vtsykun",
            "email": "vtsykun@okvpn.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Okvpn\\Bundle\\DatadogBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "Okvpn\\Bundle\\DatadogBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php":">=7.4",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "graze/dog-statsd": "^0.4 || ^1.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.7",
        "phpunit/phpunit": "^8.5 || ^9.0 || ^10.0",
        "symfony/browser-kit": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "symfony/console": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "symfony/var-dumper": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.0 || ^7.0",
        "symfony/flex": "^1.10 || ^2.0",
        "symfony/monolog-bundle": "^3.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        },
        "symfony": {
            "allow-contrib": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
